			<invoke type="java">
				<content>
					<foo></foo>
				</content>
				<finalize>
					<script>dump(_event);</script>
				</finalize>
			</invoke>
			<transition event="*" target="invokeDOM" />
		</state>
		<state id="invokeDOM">
			<invoke type="java">
				<content>
					<foo></foo>
				</content>
				<finalize>
					<script>dump(_event);</script>
				</finalize>
			</invoke>
			<transition event="*" target="invokeNull" />
		</state>
