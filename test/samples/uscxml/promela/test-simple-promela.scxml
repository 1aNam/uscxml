<scxml datamodel="promela">
  <datamodel>
    <data>
      bit v1, v2, v3, v4, v5;
    </data>
  </datamodel>
  <state id="init">
    <onentry>
      <script>
        v2=1; v3=1;
      </script>
    </onentry>
    <transition target="pass" cond="v2==(v3==v4)">
    </transition>
    <transition target="fail" />
  </state>
  <final id="pass">
    <onentry>
      <log label="Outcome" expr="'pass'"/>
    </onentry>
  </final>
  <final id="fail">
    <onentry>
      <log label="Outcome" expr="'fail'"/>
    </onentry>
  </final>
</scxml>